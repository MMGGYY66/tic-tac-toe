{
  "name": "tic-tac-toe",
  "version": "0.1.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "format:lint": "npm run format && npm run lint",
    "clear": "rimraf dist .parcel-cache"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "babel-preset-nano-react-app": "^0.1.0",
    "parcel": "^2.8.3",
    "vite": "^3.2.1"
  },
  "babel": {
    "presets": [
      "nano-react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        },
        "yt1"
      ],
      [
        "@babel/plugin-proposal-private-methods",
        {
          "loose": true
        },
        "yt2"
      ]
    ]
  }
}
