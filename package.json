{
  "name": "tic-tac-toe",
  "version": "0.1.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "format:lint": "npm run format && npm run lint",
      "predeploy": "rm -rf dist && vite build",
      "deploy": "gh-pages -d build",
    "clear": "rimraf dist parcel-cache"
  },
    "homepage": "https://MMGGYY66.github.io/ tic-tac-toe/",
  "dependencies": {
    "node-sass": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.58.2"
  },
  "devDependencies": {
    "babel-preset-nano-react-app": "^0.1.0",
    "gh-pages": "^5.0.0",
    "parcel": "^2.8.3",
    "vite": "^3.2.1"
  },
  "babel": {
    "presets": [
      "nano-react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        },
        "yt1"
      ],
      [
        "@babel/plugin-proposal-private-methods",
        {
          "loose": true
        },
        "yt2"
      ]
    ]
  }
}
